!映射几个按键
remove lock = Caps_Lock
keycode 66 = Escape
add lock = Caps_Lock
remove mod2 = Num_Lock
keycode 77 = ISO_Next_Group
add mod2 = Num_Lock

keycode 78 = F12
keycode 96 = F13

