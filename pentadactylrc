"1.0rc1

loadplugins '\.(js|penta)$'
group user

set titlestring=YeLee
set editor=Terminal\ -x\ vim
set nextpattern=(下|后)一?(页|頁|章|篇|张),\bnext|more,^>$,^(>>|»)$,^(>|»),(>|»)$
set previouspattern=(上|前)一?(页|頁|章|篇|张),\bprev|previous\b,^<$,^(<<|«)$,^(<|«),(<|«)$
imap <C-n> <Esc>:tabnext<CR>
imap <C-p> <Esc>:tabprev<CR>
nmap eh H
nmap el L
nmap ej <C-p>
nmap ek <C-n>
nmap ez ZZ
nmap ef F

" vim: set ft=pentadactyl:
